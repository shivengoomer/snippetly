{
  "manifest_version": 3,
  "name": "Snippetly",
  "version": "1.0",
  "description": "Save and organize snippets, including clipboard history and code snippets.",
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"},
  "background": {
    "service_worker": "background.js"
  }
}
